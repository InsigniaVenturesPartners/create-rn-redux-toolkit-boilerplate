  "version": "0.0.1",
  "author": "Roy Nguyen <royvn612@gmail.com>",
  "description": "React Native - Redux toolkit tree - boilerplate",
  "keywords": [
    "redux-toolkit",
    "redux-toolkit-structure",
    "react-native",
    "redux-thunk",
    "boilerplate",
    "react native template",
    "react"
  ],
  "repository": {
    "url": "https://github.com/royvn612/create-rn-redux-toolkit-boilerplate",
    "type": "git"
  },
  "bin": {
    "create-rn-redux-toolkit-boilerplate": "bin/generate-app.js"
  },
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "clean": "react-native-clean-project",
    "clean-all": "npx react-native clean-project-auto",
    "prettier:write": "npx prettier --write **/*.{js,ts,tsx}",
    "lint": "tsc --noEmit && eslint ./src --ext .js,.jsx,.ts,.tsx --fix",
    "build-staging:android": "cd android && ENVFILE=.env.staging ./gradlew assembleStagingRelease",
    "run-staging:android": "ENVFILE=.env.staging react-native run-android --variant=stagingrelease",
    "build-production:android": "cd android && ENVFILE=.env.production ./gradlew assembleRelease",
    "run-production:android": "ENVFILE=.env.production react-native run-android --variant=release",
    "generate": "node ./tools/generate.js",
    "prepare": "husky install"
  },
